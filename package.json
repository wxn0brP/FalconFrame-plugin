{
	"name": "@wxn0brp/falcon-frame-plugin",
	"version": "0.0.4",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"description": "Plugin system for FalconFrame",
	"repository": {
		"type": "git",
		"url": "https://github.com/wxn0brP/FalconFrame-plugin.git"
	},
	"homepage": "https://github.com/wxn0brP/FalconFrame-plugin",
	"author": "wxn0brP",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "tsc && tsc-alias"
	},
	"keywords": [
		"falcon-frame",
		"plugin"
	],
	"devDependencies": {
		"@types/bun": "*",
		"@wxn0brp/falcon-frame": "^0.7.0",
		"tsc-alias": "*",
		"typescript": "*"
	},
	"peerDependencies": {
		"@wxn0brp/falcon-frame": ">=0.7.0"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/*.js",
			"default": "./dist/*.js"
		}
